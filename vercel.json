{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    },
    {
      "src": "api/**/*.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    { "src": "/api/partner-center$", "dest": "/api/partner-center/status.ts" },
    { "src": "/api/partner-center/(.*)$", "dest": "/api/partner-center/$1.ts" },
    { "handle": "filesystem" },
    { "src": "/((?!api).*)", "dest": "/index.html" }
  ]
}

